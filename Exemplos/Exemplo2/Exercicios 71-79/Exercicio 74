    <script>
        window.onload = function(){
             arraiDeNumeros= [0];
            for(var i = 0; i < 10; i++)
            if(i%2 == 0)
            {
                arraiDeNumeros[i] = 5
            }else{
                arraiDeNumeros[i]=3
            }
            
        console.log(arraiDeNumeros);}
            
    </script>
