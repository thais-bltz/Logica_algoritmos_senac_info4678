    <script>
        window.onload = function(){
             var a= [];
            for(var i = 0; i < 15; i++){
           var valor = Number(prompt("Digite um numero"));
           a.push(valor);
            }console.log(a);
            for(var i = 0; i < a.length; i ++){
                if(a[i] % 10 === 0){
                    console.log("O numero" + a[i] + " é multiplo de 10 e esta na posição " + i);
                }
            }}
        
    </script>
