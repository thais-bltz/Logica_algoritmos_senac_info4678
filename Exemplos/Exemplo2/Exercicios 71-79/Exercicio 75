<body>
    
    <script>
        window.onload = function(){
             var numeros= [];
            for(var i = 0; i < 15; i++){
                if(i < 2)
                {
                    numeros[i] = 1
                }
                else{
                    numeros[i] = numeros[i-1] + numeros [i-2];
                }
            }
            console.log(numeros);        
    }
            
    </script>
</body>
