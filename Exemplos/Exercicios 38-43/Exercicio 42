    <script>
        window.onload = function(){
    var teste = Number(prompt("DigiteUmValor"));

    var todos = "";
    for(var i = 0; i <= teste; i ++){
        todos = todos +"  "+ i;
            }
            todos = todos + " Acabou!";
        alert(todos)
    }
    </script>
