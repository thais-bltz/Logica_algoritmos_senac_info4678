    <br><input type="text" id="ANO">

    <button onclick="ANO()">Click Aqui </button>
    <script>
         function ANO(){
            var ANO = document.getElementById("ANO").value;
        
        if(ANO %4 == 0)
        {
            if(ANO %100 != 0)
            {
                alert("ANO BISSEXTO");
            }
            else
            {
                if(ANO %400 == 0)
                {
                    alert("ANO BISSEXTO");
                }  
                else
                {
                    alert("ANO");
                }
            }
        }
        else
        {
            alert("ANO");
        }
    }
    </script>
