<body>
    <input type="text" id="km">
    <input type="text" id="dias">
    <button onclick="ExibeDadoDigitado()">Click Aqui</button>
<script>
  function ExibeDadoDigitado(){
    var km = document.getElementById("km").value;
    var dias = document.getElementById("dias").value;
    var valor = 90;
    var preco = 0.20;
    var resultado = Number(km * preco) + Number(dias * valor);
            alert("  " + " = " + resultado)
        }
</script>
