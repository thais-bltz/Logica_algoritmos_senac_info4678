<body>
    <input type="text" id="numero">
    <button onclick="ExibeDadoDigitado()">Click Aqui</button>
    <script>
        function ExibeDadoDigitado(){
            var valor = document.getElementById("numero").value;
            var km = valor / 10;
            var hm = km / 10;
            var dam = hm / 10;
            var dm = dam * 10;
            var cm = dm * 10;
            var mm = cm * 10;
            alert ("A Dist√¢ncia de " + valor + " corresponde a: " + km + " , " + hm + " , " + dam + " , " + dm + " , " + cm + " , " + mm)
        }
    
    </script>
</body>
